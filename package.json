{
  "name": "next-nest-example",
  "version": "0.0.1",
  "description": "",
  "module": "true",
  "scripts": {
    "mongo:start": "brew services start mongodb-community@6.0",
    "mongo:background-start": "mongod --config /opt/homebrew/etc/mongod.conf --fork",
    "mongo:stop": "brew services stop mongodb-community@6.0",
    "prebuild": "rimraf dist",
    "build": "yarn build:next && yarn build:nest",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.server.json",
    "build:scss": "yarn typed-scss-modules src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/server/main.js",
    "start:next": "next dev",
    "start:dev": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch --path tsconfig.server.json",
    "start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
    "start:docker": "docker run --rm -p 3000:3000 -p 7655:22 -it $(docker build --build-arg ssh_pub_key=\"$(cat ~/.ssh/id_rsa.pub)\" -q .)",
    "start:ssh": "ssh -o StrictHostKeyChecking=no -L 9221:localhost:9229 root@localhost -p 7655",
    "start:prod": "node dist/main",
    "start:proxy": "docker run --name nest-next-blog-proxy -v $(pwd)/nginx.conf:/etc/nginx/conf.d/default.conf:ro --add-host localnode:$(ifconfig en0 | grep inet | grep -v inet6 | awk '{print $2}') -p 8080:8080 -d nginx",
    "lint": "eslint \"{src,apps,libs,test}/**/*.tsx?\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "mongoose": "^6.8.3",
    "nest-next": "10.0.0",
    "next": "12.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "css-loader": "^6.7.3",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-node-externals": "^3.0.0"
  }
}
